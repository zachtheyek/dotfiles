[pull]
	rebase = true
[credential]
	helper = store
[init]
	defaultBranch = master
[core]
    pager = delta 
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true # use n & N to move between diff sections
    side-by-side = true
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
